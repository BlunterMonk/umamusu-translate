            "pathId": 1716830396655880609,
            "blockIdx": 7
        },
        {
            "jpName": "カツラギエース",
            "enName": "Katsuragi Ace",
            "jpText": "――ルドルフ！　シービー！",
            "enText": "Rudolf! C.B.!",
            "nextBlock": 9,
            "origClipLength": 62,
            "voiceIdx": 7,
            "animData": [
                {
                    "origLen": 93,
                    "pathId": 2250420326815990870
                },
                {
                    "origLen": 20,
