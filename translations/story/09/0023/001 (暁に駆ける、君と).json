            "jpName": "シービー＆エース",
            "enName": "C.B. & Ace",
            "jpText": "――ふっ。\n――ははっ！",
            "enText": "Ha ha!",
            "nextBlock": 34,
            "origClipLength": 39,
            "voiceIdx": 31,
            "animData": [
                {
                    "origLen": 120,
                    "pathId": -8874257585687001229
                },
                {
                    "origLen": 63,
                    "pathId": -6050818853074493230
                },
                {
                    "origLen": 116,
                    "pathId": -8866770755956785302
