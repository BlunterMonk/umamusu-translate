                }
            ],
            "pathId": 4148868931213018000,
            "blockIdx": 37
        },
        {
            "jpName": "キングヘイロー",
            "enName": "",
            "jpText": "フレーッ！　フレーッ！！\nトー・レー・セー・ンーッ！！",
            "enText": "",
            "nextBlock": 39,
            "origClipLength": 183,
            "animData": [
                {
                    "origLen": 70,
                    "pathId": -4622496624260250058
                }
