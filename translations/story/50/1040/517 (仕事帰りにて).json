            "blockIdx": 3
        },
        {
            "jpName": "ゴールドシチー",
            "enName": "Gold City",
            "jpText": "うっそ、メチャクチャ乗り過ごしてんじゃん！",
            "enText": "Oh, no, you're missing the boat!",
            "nextBlock": 5,
            "pathId": -5455852088894357451,
            "blockIdx": 4
