            "choices": [
                {
                    "jpText": "ゆっくり休むんだぞ",
                    "enText": "Get some rest.",
                    "nextBlock": 2
                },
                {
                    "jpText": "ゆっくり休んでね",
                    "enText": "Get some rest.",
