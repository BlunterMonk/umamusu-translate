            "pathId": 1997477095593721126,
            "blockIdx": 6
        },
        {
            "jpName": "マルゼンスキー",
            "enName": "Maruzensky",
            "jpText": "ええ、もちろん！",
            "enText": "Yes, of course!",
            "nextBlock": 8,
            "pathId": -1858294100262589344,
            "blockIdx": 7
        },
        {
            "jpName": "マルゼンスキー",
